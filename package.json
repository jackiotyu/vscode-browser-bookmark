{
  "publisher": "jackiotyu",
  "name": "browser-bookmark",
  "displayName": "Browser Bookmark",
  "description": "Using browser's bookmark in VSCodeüéâ Âú®VSCode‰∏≠‰ΩøÁî®ÊµèËßàÂô®‰π¶Á≠æ",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.79.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackiotyu/vscode-browser-bookmark.git"
  },
  "bugs": {
    "url": "https://github.com/jackiotyu/vscode-browser-bookmark/issues"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "bookmark",
    "chrome bookmark",
    "browse bookmark",
    "open bookmark",
    "ÊµèËßàÂô®‰π¶Á≠æ"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "browser-bookmark.openLink",
        "title": "%cmd.openLink%"
      },
      {
        "command": "browser-bookmark.copyLink",
        "title": "%cmd.copyLink%"
      },
      {
        "command": "browser-bookmark.openInternal",
        "title": "%cmd.openInternal%"
      },
      {
        "command": "browser-bookmark.openExternal",
        "title": "%cmd.openExternal%"
      },
      {
        "command": "browser-bookmark.refresh",
        "title": "%cmd.refresh%",
        "icon": "$(refresh)"
      },
      {
        "command": "browser-bookmark.search",
        "title": "%cmd.search%",
        "icon": "$(search)"
      },
      {
        "command": "browser-bookmark.openSetting",
        "title": "%cmd.openSetting%",
        "icon": "$(gear)"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "when": "view == BrowserBookmark.bookmark && viewItem == browser-bookmark.url",
          "command": "browser-bookmark.copyLink"
        },
        {
          "when": "view == BrowserBookmark.bookmark && viewItem == browser-bookmark.url",
          "command": "browser-bookmark.openInternal"
        },
        {
          "when": "view == BrowserBookmark.bookmark && viewItem == browser-bookmark.url",
          "command": "browser-bookmark.openExternal"
        }
      ],
      "view/title": [
        {
          "when": "view == BrowserBookmark.bookmark",
          "command": "browser-bookmark.refresh",
          "group": "navigation@1"
        },
        {
          "when": "view == BrowserBookmark.bookmark",
          "command": "browser-bookmark.search",
          "group": "navigation@2"
        },
        {
          "when": "view == BrowserBookmark.bookmark",
          "command": "browser-bookmark.openSetting",
          "group": "navigation@100"
        }
      ],
      "commandPalette": [
        {
          "command": "browser-bookmark.openLink",
          "when": "false"
        },
        {
          "command": "browser-bookmark.copyLink",
          "when": "false"
        },
        {
          "command": "browser-bookmark.openInternal",
          "when": "false"
        },
        {
          "command": "browser-bookmark.openExternal",
          "when": "false"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "images/icon.svg",
          "id": "BrowserBookmark",
          "title": "Browser Bookmark"
        }
      ]
    },
    "views": {
      "BrowserBookmark": [
        {
          "id": "BrowserBookmark.bookmark",
          "name": "bookmark"
        }
      ]
    },
    "keybindings": {
      "mac": "Cmd+Shift+v",
      "win": "Ctrl+Shift+v",
      "linux": "Ctrl+Shift+v",
      "command": "browser-bookmark.search"
    },
    "configuration": {
      "title": "browser-bookmark",
      "properties": {
        "browser-bookmark.defaultOpenWith": {
          "scope": "application",
          "enum": [
            "internal",
            "external"
          ],
          "enumDescriptions": [
            "%config.defaultOpenWith.enumDescriptions.internal%",
            "%config.defaultOpenWith.enumDescriptions.external%"
          ],
          "default": "external",
          "description": "%config.defaultOpenWith.description%"
        },
        "browser-bookmark.internalBrowserOpenCommand": {
          "scope": "application",
          "type": "string",
          "default": "simpleBrowser.api.open",
          "description": "%config.internalBrowserOpenCommand.description%"
        },
        "browser-bookmark.path.win.chrome": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.win.chrome.description%"
        },
        "browser-bookmark.path.mac.chrome": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.mac.chrome.description%"
        },
        "browser-bookmark.path.linux.chrome": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.linux.chrome.description%"
        },
        "browser-bookmark.path.win.edge": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.win.edge.description%"
        },
        "browser-bookmark.path.mac.edge": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.mac.edge.description%"
        },
        "browser-bookmark.path.linux.edge": {
          "scope": "machine",
          "type": "string",
          "default": "",
          "description": "%config.path.linux.edge.description%"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/ini": "^1.3.31",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/sql.js": "^1.4.4",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.16",
    "fuse.js": "^6.6.2",
    "ini": "^4.1.1",
    "open": "^9.1.0",
    "sql.js": "^1.8.0"
  }
}
